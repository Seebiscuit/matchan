{
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.0",
    "@ant-design/icons": "^5.3.0",
    "@auth/prisma-adapter": "^2.7.4",
    "@prisma/client": "^6.0.1",
    "@tanstack/react-query": "^5.22.0",
    "antd": "^5.14.0",
    "libphonenumber-js": "^1.11.16",
    "next": "^14.2.18",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/uuid": "^10.0.0",
